
public class Student {
    
    private String name;
    private String course;
    private int age;

    public void setName(String name) {
        this.name = name;
    }

    public void setCourse(String course) {
        this.age = age;
    }

    public void setAge(int age) {
        this.course = course;
    }

    public String getName() {
        return name;
    }

    public int getCourse() {
        return course;
    }

    public String getAge() {
        return age;
    }
}

